{% redirect '/new-url' %}
{% requireEdition 'pro' %}
{% requireGuest %}

{# Tags with specific syntax implementations #}
{% script %}
  console.log('Hello world');
{% endscript %}

{% script at head %}
  console.log('In head');
{% endscript %}

{% script at head with {async: true, defer: true} %}
  console.log('With attributes');
{% endscript %}

